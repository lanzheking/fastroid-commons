<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#527AC4"></solid>
            <!-- 线的宽度，颜色灰色 -->
            <stroke android:width="2dp" android:color="#527AC4"></stroke>
            <!-- 矩形的圆角半径 -->
            <corners android:radius="50dp" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#527AC4"></solid>
            <!-- 线的宽度，颜色灰色 -->
            <stroke android:width="2dp" android:color="#527AC4"></stroke>
            <!-- 矩形的圆角半径 -->
            <corners android:radius="50dp" />
        </shape>
    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <!-- 填充颜色 -->
            <solid android:color="#5781d0"></solid>
            <!-- 线的宽度，颜色灰色 -->
            <stroke android:width="2dp" android:color="#5781d0"></stroke>
            <!-- 矩形的圆角半径 -->
            <corners android:radius="50dp" />
        </shape>
    </item>


</selector>